<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @org.name %>
</p>

<p>
  <strong>Tagline:</strong>
  <%= @org.tagline %>
</p>

<p>
  <strong>Bio:</strong>
  <%= @org.bio %>
</p>

<p>
  <strong>Fbid:</strong>
  <%= @org.fbID %>
</p>

<p>
  <strong>Category:</strong>
  <%= @org.category %>
</p>

<p>
  <strong>Website:</strong>
  <%= @org.website %>
</p>

<p>
  <strong>Photo url:</strong>
  <%= @org.photo_url %>
</p>

<h3>Admins:</h3>
<ul>
  <% @org.users.each do |user| %>
    <li><%= user.full_name %></li>
  <% end %>
</ul>

<h3>Upcoming Events:</h3>
<ul>
  <% @org.events.each do |event| %>
    <li><%= link_to event.title, event %> on <%= event.start_date %></li>
  <% end %>
</ul>

<%= link_to 'Edit', edit_org_path(@org) %> |
<%= link_to 'Back', orgs_path %>
