<% unless notice.blank? %>
  <div class="alert alert-success" role="alert">
    <%= notice %>
  </div>
<% end %>

<div class="col-md-8">
  <div class="row">
    <h1 class="pill">Organizations</h1>
  </div>
  <div class="row">
    <div class="col-sm-6 list-search">
      <input type="text" class="form-control" value="Search will go hereâ€¦">
    </div>
    <div class="col-sm-6 list-categories">
      <select class="form-control filter" disabled>
        <option>Filter Organizations by Category</option disabled>
      </select>
    </div>
  </div>

  <div class="row">
    <div class="panel panel-default filled list-all">
      <% @orgs.each do |org| %>
        <div class="row">
          <%= link_to org.name, org %>
        </div>
      <% end %>
    </div>
  </div>
</div>
