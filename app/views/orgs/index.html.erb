<p id="notice"><%= notice %></p>

<h1 class="pill">Organizations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Tagline</th>
      <th>Bio</th>
      <th>Fbid</th>
      <th>Category</th>
      <th>Website</th>
      <th>Photo url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orgs.each do |org| %>
      <tr>
        <td><%= org.name %></td>
        <td><%= org.tagline %></td>
        <td><%= org.bio %></td>
        <td><%= org.fbID %></td>
        <td><%= org.category %></td>
        <td><%= org.website %></td>
        <td><%= org.photo_url %></td>
        <td><%= link_to 'Show', org %></td>
        <td><%= link_to 'Edit', edit_org_path(org) %></td>
        <td><%= link_to 'Destroy', org, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Org', new_org_path %>
